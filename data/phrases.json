  "phrases": [
    {
      "id": "phrase_001",
      "categories": ["greeting", "introduction"],
      "translations": {
        "en": {
          "text": "Hello, how are you today?",
          "phonetic": "heh-LOH, how ar yoo tuh-DAY"
        },
        "zu": {
          "text": "Sawubona, unjani namhlanje?",
          "phonetic": "sah-woo-BOH-nah, oon-JAH-nee nahm-HLAHN-jeh",
          "tts_pronunciation": "sah-woo-boh-nah, oon-jah-nee nahm-lahn-jeh"
        },
        "xh": {
          "text": "Molo, unjani namhlanje?",
          "phonetic": "MOH-loh, oon-JAH-nee nahm-HLAHN-jeh",
          "tts_pronunciation": "moh-loh, oon-jah-nee nahm-lahn-jeh"
        },
        "af": {
          "text": "Hallo, hoe gaan dit vandag met jou?",
          "phonetic": "HAH-loh, hoo khahn dit fun-DAKH met yoh"
        },
        "nso": {
          "text": "Thobela, o bjang lehono?",
          "phonetic": "toh-BEH-lah, oh BYANG leh-HOH-noh",
          "tts_pronunciation": "toh-beh-lah, oh byang leh-hoh-noh"
        }
      }
    },
    {
      "id": "phrase_002",
      "categories": ["introduction"],
      "translations": {
        "en": {
          "text": "My name is... I am a healthcare worker.",
          "phonetic": "my naym iz... I am uh HELTH-kair WUR-ker"
        },
        "zu": {
          "text": "Igama lami ngu... Ngingumsebenzi wezempilo.",
          "phonetic": "ee-GAH-mah LAH-mee ngoo... ngee-ngoo-m-seh-BEHN-zee weh-zehm-PEE-loh",
          "tts_pronunciation": "ee-gah-mah lah-mee ngoo, ngee-ngoo-m-seh-behn-zee weh-zehm-pee-loh"
        },
        "xh": {
          "text": "Igama lam ngu... Ndingumsebenzi wezempilo.",
          "phonetic": "ee-GAH-mah lahm ngoo... n-dee-ngoo-m-seh-BEHN-zee weh-zehm-PEE-loh",
          "tts_pronunciation": "ee-gah-mah lahm ngoo, n-dee-ngoo-m-seh-behn-zee weh-zehm-pee-loh"
        },
        "af": {
          "text": "My naam is... Ek is 'n gesondheidswerker.",
          "phonetic": "may nahm is... ek is un kheh-SOND-hayts-vair-ker"
        },
        "nso": {
          "text": "Leina la ka ke... Ke mo코omi wa t코a maphelo.",
          "phonetic": "LEH-nah lah kah keh... keh moh-SHOH-mee wah chah mah-PEH-loh",
          "tts_pronunciation": "leh-nah lah kah keh, keh moh-shoh-mee wah chah mah-peh-loh"
        }
      }
    },
    {
      "id": "phrase_003",
      "categories": ["introduction"],
      "translations": {
        "en": {
          "text": "Please tell me your name.",
          "phonetic": "pleez tel mee yor naym"
        },
        "zu": {
          "text": "Ngicela ungitshele igama lakho.",
          "phonetic": "ngee-CHEH-lah oon-gee-TSHEH-leh ee-GAH-mah LAH-koh",
          "tts_pronunciation": "ngee-cheh-lah oon-gee-tsheh-leh ee-gah-mah lah-koh"
        },
        "xh": {
          "text": "Nceda undixelele igama lakho.",
          "phonetic": "n-CHEH-dah oon-dee-kseh-LEH-leh ee-GAH-mah LAH-koh",
          "tts_pronunciation": "n-cheh-dah oon-dee-kseh-leh-leh ee-gah-mah lah-koh"
        },
        "af": {
          "text": "S칡 my asseblief jou naam.",
          "phonetic": "say may AH-seh-bleef yoh nahm"
        },
        "nso": {
          "text": "Hle mpolele leina la gago.",
          "phonetic": "hleh m-poh-LEH-leh LEH-nah lah GAH-goh",
          "tts_pronunciation": "hleh m-poh-leh-leh leh-nah lah gah-goh"
        }
      }
    },
    {
      "id": "phrase_004",
      "categories": ["symptoms", "pain_assessment"],
      "translations": {
        "en": {
          "text": "Where does it hurt?",
          "phonetic": "wair duz it hurt"
        },
        "zu": {
          "text": "Kubuhlungu kuphi?",
          "phonetic": "koo-boo-HLOO-ngoo KOO-pee",
          "tts_pronunciation": "koo-boo-hloo-ngoo koo-pee"
        },
        "xh": {
          "text": "Kubuhlungu phi?",
          "phonetic": "koo-boo-HLOO-ngoo pee",
          "tts_pronunciation": "koo-boo-hloo-ngoo pee"
        },
        "af": {
          "text": "Waar is die pyn?",
          "phonetic": "vahr is dee payn"
        },
        "nso": {
          "text": "Go hlaba kae?",
          "phonetic": "khoh HLAH-bah kah-eh",
          "tts_pronunciation": "khoh hlah-bah kah-eh"
        }
      }
    },
    {
      "id": "phrase_005",
      "categories": ["symptoms"],
      "translations": {
        "en": {
          "text": "When did this start?",
          "phonetic": "wen did this start"
        },
        "zu": {
          "text": "Kuqale nini lokhu?",
          "phonetic": "koo-KAH-leh NEE-nee LOH-koo",
          "tts_pronunciation": "koo-kah-leh nee-nee loh-koo"
        },
        "xh": {
          "text": "Kuqale nini oku?",
          "phonetic": "koo-KAH-leh NEE-nee OH-koo",
          "tts_pronunciation": "koo-kah-leh nee-nee oh-koo"
        },
        "af": {
          "text": "Wanneer het dit begin?",
          "phonetic": "vah-NEER het dit beh-KHIN"
        },
        "nso": {
          "text": "Se se thomile neng?",
          "phonetic": "seh seh toh-MEE-leh nehng",
          "tts_pronunciation": "seh seh toh-mee-leh nehng"
        }
      }
    },
    {
      "id": "phrase_006",
      "categories": ["instructions"],
      "translations": {
        "en": {
          "text": "Please sit down here.",
          "phonetic": "pleez sit down heer"
        },
        "zu": {
          "text": "Ngicela uhlale phansi lapha.",
          "phonetic": "ngee-CHEH-lah oo-HLAH-leh PAHN-see LAH-pah",
          "tts_pronunciation": "ngee-cheh-lah oo-hlah-leh pahn-see lah-pah"
        },
        "xh": {
          "text": "Nceda uhlale phantsi apha.",
          "phonetic": "n-CHEH-dah oo-HLAH-leh PAHN-tsee AH-pah",
          "tts_pronunciation": "n-cheh-dah oo-hlah-leh pahn-tsee ah-pah"
        },
        "af": {
          "text": "Sit asseblief hier.",
          "phonetic": "sit AH-seh-bleef heer"
        },
        "nso": {
          "text": "Hle o dule mo.",
          "phonetic": "hleh oh DOO-leh moh",
          "tts_pronunciation": "hleh oh doo-leh moh"
        }
      }
    },
    {
      "id": "phrase_007",
      "categories": ["examination", "instructions"],
      "translations": {
        "en": {
          "text": "I need to examine you.",
          "phonetic": "I need too ig-ZAM-in yoo"
        },
        "zu": {
          "text": "Kudingeka ngikuhlole.",
          "phonetic": "koo-dee-NGEH-kah ngee-koo-HLOH-leh",
          "tts_pronunciation": "koo-dee-ngeh-kah ngee-koo-hloh-leh"
        },
        "xh": {
          "text": "Kufuneka ndikuhlole.",
          "phonetic": "koo-foo-NEH-kah n-dee-koo-HLOH-leh",
          "tts_pronunciation": "koo-foo-neh-kah n-dee-koo-hloh-leh"
        },
        "af": {
          "text": "Ek moet jou ondersoek.",
          "phonetic": "ek moot yoh ON-der-sook"
        },
        "nso": {
          "text": "Ke swanet코e go go hlahloba.",
          "phonetic": "keh swah-NEH-tsheh khoh khoh hlah-HLOH-bah",
          "tts_pronunciation": "keh swah-neh-tsheh khoh khoh hlah-hloh-bah"
        }
      }
    },
    {
      "id": "phrase_008",
      "categories": ["examination", "instructions"],
      "translations": {
        "en": {
          "text": "Please open your mouth.",
          "phonetic": "pleez OH-pen yor mouth"
        },
        "zu": {
          "text": "Ngicela uvule umlomo wakho.",
          "phonetic": "ngee-CHEH-lah oo-VOO-leh oom-LOH-moh WAH-koh",
          "tts_pronunciation": "ngee-cheh-lah oo-voo-leh oom-loh-moh wah-koh"
        },
        "xh": {
          "text": "Nceda uvule umlomo wakho.",
          "phonetic": "n-CHEH-dah oo-VOO-leh oom-LOH-moh WAH-koh",
          "tts_pronunciation": "n-cheh-dah oo-voo-leh oom-loh-moh wah-koh"
        },
        "af": {
          "text": "Maak asseblief jou mond oop.",
          "phonetic": "mahk AH-seh-bleef yoh mond ohp"
        },
        "nso": {
          "text": "Hle o bule molomo wa gago.",
          "phonetic": "hleh oh BOO-leh moh-LOH-moh wah GAH-goh",
          "tts_pronunciation": "hleh oh boo-leh moh-loh-moh wah gah-goh"
        }
      }
    },
    {
      "id": "phrase_009",
      "categories": ["reassurance"],
      "translations": {
        "en": {
          "text": "Don't worry, we will help you.",
          "phonetic": "dohnt WUR-ee, wee will help yoo"
        },
        "zu": {
          "text": "Ungakhathazeki, sizokusiza.",
          "phonetic": "oo-NGAH-kah-tah-ZEH-kee, see-zoh-koo-SEE-zah",
          "tts_pronunciation": "oo-ngah-kah-tah-zeh-kee, see-zoh-koo-see-zah"
        },
        "xh": {
          "text": "Musa ukukhathazeka, siza kukunceda.",
          "phonetic": "MOO-sah oo-koo-kah-tah-ZEH-kah, SEE-zah koo-koon-CHEH-dah",
          "tts_pronunciation": "moo-sah oo-koo-kah-tah-zeh-kah, see-zah koo-koon-cheh-dah"
        },
        "af": {
          "text": "Moenie bekommerd wees nie, ons sal jou help.",
          "phonetic": "MOO-nee beh-KOM-merd vays nee, ons sahl yoh help"
        },
        "nso": {
          "text": "O se tshwenye, re tla go thu코a.",
          "phonetic": "oh seh TSHWEN-yeh, reh tlah khoh TOO-shah",
          "tts_pronunciation": "oh seh tshwen-yeh, reh tlah khoh too-shah"
        }
      }
    },
    {
      "id": "phrase_010",
      "categories": ["emergency"],
      "translations": {
        "en": {
          "text": "This is an emergency. We need to act quickly.",
          "phonetic": "this iz an ee-MUR-jen-see. wee need too akt KWIK-lee"
        },
        "zu": {
          "text": "Lokhu kuyisimo esiphuthumayo. Kudingeka sisebenze ngokushesha.",
          "phonetic": "LOH-koo koo-yee-SEE-moh eh-see-poo-too-MAH-yoh. koo-dee-NGEH-kah see-seh-BEHN-zeh ngoh-koo-SHEH-shah",
          "tts_pronunciation": "loh-koo koo-yee-see-moh eh-see-poo-too-mah-yoh. koo-dee-ngeh-kah see-seh-behn-zeh ngoh-koo-sheh-shah"
        },
        "xh": {
          "text": "Le yimeko ephuthumayo. Kufuneka sisebenze ngokukhawuleza.",
          "phonetic": "leh yee-MEH-koh eh-poo-too-MAH-yoh. koo-foo-NEH-kah see-seh-BEHN-zeh ngoh-koo-kah-woo-LEH-zah",
          "tts_pronunciation": "leh yee-meh-koh eh-poo-too-mah-yoh. koo-foo-neh-kah see-seh-behn-zeh ngoh-koo-kah-woo-leh-zah"
        },
        "af": {
          "text": "Dit is 'n noodgeval. Ons moet vinnig optree.",
          "phonetic": "dit is un NOHT-kheh-fahl. ons moot FIN-nikh OP-tray"
        },
        "nso": {
          "text": "Se ke tiragalo ya kapetepete. Re swanet코e go 코oma ka pela.",
          "phonetic": "seh keh tee-rah-GAH-loh yah kah-peh-teh-PEH-teh. reh swah-NEH-tsheh khoh SHOH-mah kah PEH-lah",
          "tts_pronunciation": "seh keh tee-rah-gah-loh yah kah-peh-teh-peh-teh. reh swah-neh-tsheh khoh shoh-mah kah peh-lah"
        }
      }
    }
  ]
